{
  "name": "airplayer",
  "version": "0.0.1",
  "description": "airplay receiver built with electron",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build": "mkdir build; electron-packager . AirPlayer --platform=darwin --arch=x64 --out=build",
    "build-win": "mkdir build; electron-packager . AirPlayer --platform=win32 --arch=x64 --out=build"
  },
  "dependencies": {
    "fix-path": "^2.1.0"
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-packager": "^14.2.0"
  }
}
